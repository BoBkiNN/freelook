  "authors": [
    "Celibistrial"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/freelook-fabric",
    "sources": "https://github.com/Celibistrial/freelook"
  },
  "license": "GNU GPLv3",
  "icon": "assets/freelook/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "freelook.freelook.client.FreeLookModClient"
    ],
    "main": [
      "freelook.freelook.FreeLookMod"
    ],
    "modmenu": [
      "freelook.freelook.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "freelook.mixins.json"
